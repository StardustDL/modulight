@inject IBlazorUIProvider UIProvider

<Icon>
    <Component>
        <ModuleIcon Module="Module">
            <Fallback>
                @if (UIProvider.DefaultModuleIcon is not null)
                {
                    @UIProvider.DefaultModuleIcon
                }
            </Fallback>
        </ModuleIcon>
    </Component>
</Icon>

@code {
    [Parameter]
    public IRazorComponentClientModule? Module { get; set; }
}
